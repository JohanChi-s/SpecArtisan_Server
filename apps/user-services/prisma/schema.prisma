datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt    DateTime  @default(now())
  email        String    @unique
  firstName    String?
  id           String    @id @default(cuid())
  isActive     Boolean?
  isAdmin      Boolean
  isViewer     Boolean?
  language     String?
  lastActiveAt DateTime? @updatedAt
  lastName     String?
  password     String
  profileId    String
  roles        Json
  updatedAt    DateTime  @updatedAt
  username     String    @unique
}
